<layout>
    <adminhtml_system_config_edit>
        <reference name="head">
            <block type="fastlycdn/adminhtml_head" name="namespace_adminhtml_head" as="namespace_adminhtml_head" />
        </reference>
        <reference name="content">
            <block type="fastlycdn/adminhtml_system_config_dialogs" name="fastly.modal"/>
        </reference>
    </adminhtml_system_config_edit>
    <adminhtml_cache_index>
        <reference name="content">
            <block type="fastlycdn/adminhtml_cache_additional" name="fastlycdn.cache.additional"
                   template="fastlycdn/cache/additional.phtml" after="cache.additional"/>
        </reference>
    </adminhtml_cache_index>
</layout>
